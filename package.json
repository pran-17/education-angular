{
  "name": "angular-starter",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build demo --configuration production",
    "backend": "cd backend && npm install && npm start",
    "deploy:docker": "docker-compose up -d",
    "deploy:build": "npm run build:prod && docker-compose up -d --build",
    "deploy:vercel": "npm run build:prod && vercel --prod",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir=dist/demo/browser"
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.2.7",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "mongoose": "^8.0.0",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "typescript": "^5.8.2"
  }
}
